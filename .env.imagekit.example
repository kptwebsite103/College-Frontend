# ImageKit Configuration
# Add these to your .env file in the frontend

# ImageKit Credentials (get from https://imagekit.io/dashboard)
VITE_IMAGEKIT_PUBLIC_KEY=your_public_key_here
VITE_IMAGEKIT_URL_ENDPOINT=https://ik.imagekit.io/your_url_endpoint

# Backend Environment Variables (add to backend .env)
IMAGEKIT_PUBLIC_KEY=your_public_key_here
IMAGEKIT_PRIVATE_KEY=your_private_key_here
IMAGEKIT_URL_ENDPOINT=https://ik.imagekit.io/your_url_endpoint

# Example:
# VITE_IMAGEKIT_PUBLIC_KEY=public_abc123xyz
# VITE_IMAGEKIT_URL_ENDPOINT=https://ik.imagekit.io/kpt103
# IMAGEKIT_PUBLIC_KEY=public_abc123xyz
# IMAGEKIT_PRIVATE_KEY=private_abc123xyz
# IMAGEKIT_URL_ENDPOINT=https://ik.imagekit.io/kpt103
